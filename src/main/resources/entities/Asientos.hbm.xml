<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.unq.persistencia.model.empresa.Asiento" table="Asiento">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="identity" />
        </id>

        <property name="numero" type="integer">
            <column name="numero" not-null="false" unique="false" />
        </property>
    </class>
</hibernate-mapping>